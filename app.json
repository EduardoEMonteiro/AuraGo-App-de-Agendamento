{
  "expo": {
    "name": "Aura",
    "displayName": "Aura",
    "slug": "aura",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/ic_logo_notification.png",
    "splash": {
      "image": "./assets/images/logo_aura.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "scheme": "meuapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "googleServicesFile": "./firebase-config/GoogleService-Info.plist"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/ic_logo_notification.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.bloom.aoo"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/logo_aura.png",
          "resizeMode": "contain",
          "backgroundColor": "#efefef"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "67f83a4c-9042-4361-9f58-f540aaa2bb3e"
      }
    },
    "owner": "eduardoemonteiro"
  }
}
